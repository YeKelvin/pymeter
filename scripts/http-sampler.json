[
  {
    "name": "http-debug-collection",
    "remark": "",
    "class": "TestCollection",
    "enabled": true,
    "property": {
      "TestCollection__serialize_groups": "true"
    },
    "children": [
      {
        "name": "变量配置器",
        "remark": "",
        "class": "VariableDataSet",
        "enabled": true,
        "property": {
          "Arguments__arguments": [
            {
              "class": "Argument",
              "property": {
                "Argument__name": "key111",
                "Argument__value": "val111"
              }
            }
          ]
        }
      },
      {
        "name": "结果收集器",
        "remark": "",
        "class": "ResultCollector",
        "enabled": true,
        "property": {}
      },
      {
        "name": "http-debug-group",
        "remark": "",
        "class": "TestGroup",
        "enabled": true,
        "property": {
          "TestGroup__on_sample_error": "continue",
          "TestGroup__number_groups": "1",
          "TestGroup__start_interval": "0",
          "TestGroup__main_controller": {
            "class": "LoopController",
            "property": {
              "LoopController__loops": "1",
              "LoopController__continue_forever": "false"
            }
          }
        },
        "children": [
          {
            "name": "http-headers-debug-configuration",
            "remark": "",
            "class": "HTTPHeaderManager",
            "enabled": true,
            "property": {
              "HeaderManager__headers": [
                {
                  "class": "HTTPHeader",
                  "property": {
                    "Header__name": "group-level-header-name",
                    "Header__value": "group-level-header-value"
                  }
                }
              ]
            }
          },
          {
            "name": "http-debug-sampler",
            "remark": "描述",
            "class": "HTTPSampler",
            "enabled": true,
            "property": {
              "HTTPSampler__url": "https://httpbin.org/get",
              "HTTPSampler__method": "GET",
              "HTTPSampler__params": {
                "class": "Arguments",
                "property": {
                  "Arguments__arguments": [
                    {
                      "class": "Argument",
                      "property": {
                        "Argument__name": "debugName",
                        "Argument__value": "${key111}",
                        "Argument__desc": "debugDesc"
                      }
                    }
                  ]
                }
              },
              "HTTPSampler__data": "",
              "HTTPSampler__files": "",
              "HTTPSampler__encoding": "UTF-8",
              "HTTPSampler__follow_redirects": "",
              "HTTPSampler__auto_redirects": "",
              "HTTPSampler__keep_alive": "",
              "HTTPSampler__connect_timeout": "",
              "HTTPSampler__response_timeout": "",
              "HTTPSampler__header_manager": {
                "class": "HTTPHeaderManager",
                "property": {
                  "HeaderManager__headers": [
                    {
                      "class": "HTTPHeader",
                      "property": {
                        "Header__name": "sampler-header-name",
                        "Header__value": "sampler-header-value"
                      }
                    },
                    {
                      "class": "HTTPHeader",
                      "property": {
                        "Header__name": "sampler-header-name-222",
                        "Header__value": "${__Random(1, 10)}"
                      }
                    }
                  ]
                }
              }
            },
            "children": null
          }
        ]
      }
    ]
  }
]